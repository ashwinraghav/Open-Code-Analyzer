<div id="supportingText">
  <div id="explanation">
    <h3>Upload your code and be ready to be judged</h3>
    <% unless flash[:error].blank? %>
        <div class="error">
          <%= flash[:error] %>
        </div>
    <% end %>


    <% form_for CodeSubmission, @code_submission_request, :url => code_submissions_path, :html =>  {:method =>"post", :multipart => "true", :name =>"uploadCode"} do |code_submission_request| %>
        <p><label for="code_submission_request_upload">Select File</label> :
          <%= file_field 'upload', 'datafile' %>
          <%= submit_tag "Upload code", :class => :input %>
    <% end %>
  </div>
</div>

<% content_for :side_links do %>
    <li><%= link_to "New Review", new_review_url(:id => params[:id], :pursue => "false") %></li>
    <li><a href="">Blah</a></li>
    <li><a href="">Blah</a></li>
<% end %>